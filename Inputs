rental = input("What kind of rental do you want? ")
print(f"Let me see if I can find you a {rental}")

group = int(input("How many people are in the dinner group? "))

if group > 8 :
    print("You will have to wait for the table")
else:
    print("The table is ready")

num = int(input("Give me a number "))
if num % 10 == 0:
    print(f"{num} is multiple of 10")
else:
    print(f"{num} is not a multiple of 10")



topps = []
print(topps)
while True:
    
    topping = input("Enter a topping: ")
    if topping != "quit":
        print(f"I will add the {topping}")
        topps.append(topping)
    else:
        break
print(topps)



while True:
    age_input = input("Please enter your age: ")
    if age_input.lower() == "quit":
        break
    else:
        age = int(age_input)
        if age < 3:
            print("The ticket is free")
            
        elif age >= 3 and age <= 12:
            print("The ticket is $10")
            
        else:
            print("The ticket is $15")

sandwich_orders = ["Egg", "pastrami","chicken", "pastrami", "Masala", "pastrami"]

finished_sandwiches = []

print("deli has ran out of pastrami")

while 'pastrami' in sandwich_orders:
    sandwich_orders.remove('pastrami')


while sandwich_orders:
    current_sandwich = sandwich_orders.pop(0)
    print(f"I made your {current_sandwich} sandwich")
    finished_sandwiches.append(current_sandwich)
    

for f in finished_sandwiches:
    print(f"{f} sandwich was made")


responses = {}

polling = True

while polling:
    user = input("What is your name? ")
    vacation = input("If you could visit one place in the world, where would you go? ")
    responses[user] = vacation

    repeat = input("Would another person like to answer? (yes/no)")
    if repeat == "no":
        polling = False
print("Poll results")
for user, vacay in responses.items():
    print(f"{user} would like to visit {vacay}")